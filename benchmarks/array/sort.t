@a = ();
while (<DATA>) {
    chop;
    push(@a, $_);
}
srand(10);
push(@b, splice(@a, rand(@a), 1)) while @a;  # shuffle

&runtest(0.15, <<'ENDTEST');

    @a = sort @b;

ENDTEST


